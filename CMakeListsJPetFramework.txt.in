cmake_minimum_required(VERSION 2.8.2)

project(JPetFramework-download NONE)

include(ExternalProject)
ExternalProject_Add(
    JPetFramework

    GIT_REPOSITORY "${GITHUB_URL}"
    GIT_TAG "${GIT_TAG}"
    UPDATE_COMMAND ""
    PATCH_COMMAND "" 
    SOURCE_DIR "${CMAKE_CURRENT_BINARY_DIR}/j-pet-framework-src"
    CMAKE_ARGS -DCMAKE_INSTALL_PREFIX=${CMAKE_CURRENT_BINARY_DIR}/j-pet-framework-build
    TEST_COMMAND "" #disable tests
  )